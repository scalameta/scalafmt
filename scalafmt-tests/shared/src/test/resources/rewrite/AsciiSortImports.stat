rewrite.rules = [AsciiSortImports]

<<< ascii sorting
import a.{
  ~>,
  lowercase,
  Uppercase,
  `symbol`
}
>>>
import a.{Uppercase, `symbol`, lowercase, ~>}
<<< ascii sorting (format:off)
// format: off
import a.{
  ~>,
  lowercase,
  Uppercase,
  `symbol`
}

>>>
// format: off
import a.{
  ~>,
  lowercase,
  Uppercase,
  `symbol`
}
