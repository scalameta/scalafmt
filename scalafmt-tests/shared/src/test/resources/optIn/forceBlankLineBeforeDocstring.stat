preset = default
<<< docstring does not force line break
docstrings.forceBlankLineBefore = false
===
object Stuff {
  /** Some function */
  def hello = ()
}
>>>
object Stuff {
  /** Some function */
  def hello = ()
}
<<< force blank line
docstrings.forceBlankLineBefore = true
===
object Stuff {
  /** Some function */
  def hello = ()
}
>>>
object Stuff {

  /** Some function */
  def hello = ()
}
<<< support deprecated name with false
optIn.forceBlankLineBeforeDocstring = false
===
object Stuff {
  /** Some function */
  def hello = ()
}
>>>
object Stuff {
  /** Some function */
  def hello = ()
}
<<< support deprecated name with true
optIn.forceBlankLineBeforeDocstring = true
===
object Stuff {
  /** Some function */
  def hello = ()
}
>>>
object Stuff {

  /** Some function */
  def hello = ()
}
