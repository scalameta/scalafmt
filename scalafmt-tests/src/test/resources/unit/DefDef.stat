implicit def xorBifunctor
    : Bitraverse[Xor] =
  new Bitraverse[Xor] {
    def bitraverse[G[_], A, B, C, D](
        /*c1*/ fab: Xor[A, B])(
        /*c2*/
        f: A => G[C],
        g: B => G[D])(implicit
        G: Applicative[G])
        : G[Xor[C, D]] =
      fab match {
        case Xor.Left(a) =>
          G.map(f(a))(Xor.left)
        case Xor.Right(b) =>
          G.map(g(b))(Xor.right)
      }
  }
