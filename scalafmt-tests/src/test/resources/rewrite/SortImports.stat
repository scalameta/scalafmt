rewrite.rules = [SortImports]
<<< basic
package a {
import a.b.{
  c,
  a,
  b
}, k.{
  g, f
}
}
>>>
package a {

  import a.b.{a, b, c}
  import k.{f, g}

}
<<< nested
object a {
import a.b.{
  c,
  b
}
}
>>>
object a {

  import a.b.{b, c}

}
<<< sorting
import a.{
  ~>,
  lowercase,
  Uppercase,
  `symbol`
}
>>>
import a.{`symbol`, ~>, lowercase, Uppercase}
<<< caveat: rename/wildcard/unimport
import a.{
  zzzz => _,
  bar
}
>>>
import a.{bar, zzzz => _}
<<< SKIP caveat: comments
import a.{
  c, // comment for c
  b
}
>>>
import a.{
  b, // comment for c
  c
}
