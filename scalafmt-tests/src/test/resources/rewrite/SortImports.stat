rewrite.rules = [SortImports]
<<< basic
import a.b.{
  c,
  a,
  b
}, k.{
  g, f
}
>>>
import a.b.{a, b, c}, k.{f, g}
<<< nested
object a {
import a.b.{
  c,
  b
}
}
>>>
object a {
  import a.b.{b, c}
}
<<< nested (format:off)
object a {
// format: off
import a.b.{
  c,
  b
}
}

>>>
object a {
// format: off
import a.b.{
  c,
  b
}
}
<<< nested (format:off partial)
object a {
import a.b.{
  c,
// format is not off
  b
}
import a.b.{
  c,
// format: off
  b
}
}

>>>
object a {
  import a.b.{
    b,
// format is not off
    c
  }
  import a.b.{
    c,
// format: off
  b
}
}
<<< sorting
import a.{
  ~>,
  lowercase,
  Uppercase,
  `symbol`
}
>>>
import a.{`symbol`, ~>, lowercase, Uppercase}
<<< caveat: rename/wildcard/unimport
import a.{
  zzzz => _,
  bar
}
>>>
import a.{zzzz => _, bar}
<<< caveat: comments
import a.{
  c, // comment for c
  b
}
>>>
import a.{
  b, // comment for c
  c
}
