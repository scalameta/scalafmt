rewrite.rules = [RedundantParens]
<<< basic
object a {
  def c(b: List[Int]): List[Int] =
    for {
      a <- b
      if ((a))
    } yield a
}
>>>
object a {
  def c(b: List[Int]): List[Int] =
    for {
      a <- b
      if a
    } yield a
}
<<< basic (format:off)
object a {
// format: off
  def c(b: List[Int]): List[Int] =
    for {
      a <- b
      if ((a))
    } yield a
}

>>>
object a {
// format: off
  def c(b: List[Int]): List[Int] =
    for {
