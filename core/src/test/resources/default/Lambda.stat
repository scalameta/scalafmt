  def isAliasTypeInner = {
    x match {
      case ta: ScTypeAlias => //higher kind case
        typesCallSubstitutor(
            ta.typeParameters.map(tp =>
                  (tp.name, ScalaPsiUtil.getPsiElementId(tp))),
            ta.typeParameters.map(tp => {
              args += new ScExistentialArgument()
              ScTypeVariable(name)
            }))
    }
  }
}
